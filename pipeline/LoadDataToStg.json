{
	"name": "LoadDataToStg",
	"properties": {
		"activities": [
			{
				"name": "CopyDataStgMachine",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT [Code]\n      ,[Name]\n      ,[Condition]\n      ,[LastModified]\n  FROM [dbo].[Machine]\n  WHERE [LastModified] >(SELECT  ISNULL(MAX([LastModified]),'1999-01-01 00:00:00.0000000') FROM [stg].[Machine])"
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Code",
									"type": "String"
								},
								"sink": {
									"name": "Code",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "Name",
									"type": "String"
								},
								"sink": {
									"name": "Name",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "Condition",
									"type": "String"
								},
								"sink": {
									"name": "Condition",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "LastModified",
									"type": "DateTime"
								},
								"sink": {
									"name": "LastModified",
									"type": "DateTime"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "InputMachine",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "OutputStgMachine",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "CopyDataStgCustomer",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "CopyDataStgMachine",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT [Code]\n      ,[Name]\n      ,[LastModified]\n  FROM [dbo].[Customer]\n  WHERE [LastModified] >(SELECT  ISNULL(MAX([LastModified]),'1999-01-01 00:00:00.0000000') FROM [stg].[Customer])"
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Code",
									"type": "String"
								},
								"sink": {
									"name": "Code",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "Name",
									"type": "String"
								},
								"sink": {
									"name": "Name",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "LastModified",
									"type": "DateTime"
								},
								"sink": {
									"name": "LastModified",
									"type": "DateTime"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "InputCustomer",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "OutputStgCustomer",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Demo_ADLS_PROJ"
		},
		"annotations": []
	}
}