{
	"name": "DeltaFileCopyPipeLine",
	"properties": {
		"description": "Will read data from file FileName_YYYY_MM_DD.csv",
		"activities": [
			{
				"name": "Copy_t6r",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "sbressmessage//"
					},
					{
						"name": "Destination",
						"value": "[dbo].[EMP]"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSetting",
							"recursive": true,
							"wildcardFileName": {
								"value": "@{concat('First_',formatDateTime(utcnow(),'yyyy'),'*.csv')}",
								"type": "Expression"
							},
							"enablePartitionDiscovery": false,
							"modifiedDatetimeStart": {
								"value": "@{concat(formatDateTime(utcnow(),'MM'),'/', formatDateTime(utcnow(),'dd'),'/',formatDateTime(utcnow(),'yyyy'),' 00:00:00')}",
								"type": "Expression"
							}
						},
						"formatSettings": {
							"type": "DelimitedTextReadSetting",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "truncate table [dbo].[emp]"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"type": "String",
									"ordinal": 1
								},
								"sink": {
									"name": "EMPNO",
									"type": "Int32"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 2
								},
								"sink": {
									"name": "ENAME",
									"type": "String"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 3
								},
								"sink": {
									"name": "JOB",
									"type": "String"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 4
								},
								"sink": {
									"name": "MGR",
									"type": "Int32"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 5
								},
								"sink": {
									"name": "HIREDATE",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 6
								},
								"sink": {
									"name": "SAL",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 7
								},
								"sink": {
									"name": "COMM",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 8
								},
								"sink": {
									"name": "DEPTNO",
									"type": "Int32"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_t6r",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_t6r",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Demo_Cloud_Cloud"
		},
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}